dleInputChange}\n          resetBtn={resetGroup}\n          sortBtn={sortNames}\n          children={nameSort === \"AZ\" ? \"Sort A–Z\" : \"Sort Z–A\"}\n          handleSelectDropdown={handleSelectDropdown} />\n\n        {/* Validation */}\n        {warning === false ? null : <h4>Woops, please use letters only. Numbers or special characters won't display results.</h4>}\n        {group.length === 0 ? <h4>Looks like we don't have this team member. Please try a different name or hit \"Reset\".</h4> : null}\n        \n          <CardWrap>\n            {group.map(person => (\n              <GroupCard\n                key={person.id}\n                name={person.name}\n                title={person.title}\n                location={person.location}\n                phone={person.phone}\n                email={person.email}\n              />\n            ))}\n          </CardWrap>\n        \n      </Main>\n    </>\n  );\n}\n\nexport default App;\n","/Users/bowerpower/Desktop/UserLog/src/components/CardWrap/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/Main/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/Header/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/Bar/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/GroupCard/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/Search/index.js",[],"/Users/bowerpower/Desktop/UserLog/src/components/BtnGroup/index.js",[],{"ruleId":"49","severity":1,"message":"50","line":6,"column":8,"nodeType":"51","messageId":"52","endLine":6,"endColumn":23},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":33,"column":6,"nodeType":"59","endLine":33,"endColumn":14,"suggestions":"60"},"no-unused-vars","'reportWebVitals' is defined but never used.","Identifier","unusedVar","no-native-reassign",["61"],"no-negated-in-lhs",["62"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'group'. 